$.getJSON('http://app.inetscada.com:8080/api/sp_GetCurrentPM01_data_Result', function(data) {
  $('text#dg81')[0].innerHTML = Math.abs((Math.round(data.total_Active_Power * 1000) / 1000)) + ' kW';
  $('text#dg82')[0].innerHTML = (Math.round(data.total_Apparent_Power * 1000) / 1000) + ' kVA';
});

